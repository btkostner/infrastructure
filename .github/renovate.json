{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "additionalReviewers": ["btkostner"],
  "branchPrefix": "renovate/",
  "dependencyDashboard": true,
  "extends": [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigests",
    "github>btkostner/infrastructure//.github/renovate/auto-merge.json",
    "github>btkostner/infrastructure//.github/renovate/changelogs.json",
    "github>btkostner/infrastructure//.github/renovate/custom-managers.json",
    "github>btkostner/infrastructure//.github/renovate/groups.json",
    "github>btkostner/infrastructure//.github/renovate/overrides.json",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":timezone(America/Phoenix)"
  ],
  "labels": ["dependencies"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "patch": {
    "enabled": false
  },
  "platformCommit": "enabled",
  "prHourlyLimit": 0
}
